@using ASP.NET.Projet.Models
@model NoteViewModel

<h2>Modifier la note</h2>

@using (Html.BeginForm())
{
    <div>
        @Html.LabelFor(m => m.Note)
        <br />
        @Html.EditorFor(m => m.Note, new { @class = "form-control" })
    </div>
    <br />
    <button class="btn btn-success" type="submit" value="Modifier">Modifier</button>
}